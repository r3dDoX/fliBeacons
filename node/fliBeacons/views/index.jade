extends layout

block content
	core-header-panel
		core-toolbar
			paper-tabs(selected="all", valueattr="name", self-end)
				paper-tab(name="monitor") Monitor
				paper-tab(name="game") Game
		.container(center)
			.tools
				paper-button#station Register base station
				paper-button#drone-far Add drone
				paper-button#drone-near Move drone
				paper-button#drone-immediate Remove drone
			#log
		#game-container(style="display: none")
			#stations
			#map
			#ranking
				fli-ranking

	
	script(src="/socket.io/socket.io.js")
	script(src="javascripts/client.js")
	script(src="javascripts/monitor.js")
	