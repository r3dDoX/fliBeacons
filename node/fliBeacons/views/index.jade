extends layout

block content
	core-header-panel
		core-toolbar
			paper-tabs(selected="all", valueattr="name", self-end)
				paper-tab(name="all") Drones
				paper-tab(name="favorites") Stations
		.container(center)
			.tools
				paper-button#drone-near Drone 'near
				paper-button#drone-far Drone 'far'
				paper-button#drone-immediate Drone 'immediate'
				paper-button#station Send station event
			#stations
			#log

	
	script(src="/socket.io/socket.io.js")
	script(src="javascripts/client.js")
	